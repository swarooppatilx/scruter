    <style>
      .social-icons a {
        transition: all .38s ease;
        font-size: 25px;
        margin-left: 10px;
        color: black;
      }
      .social-icons a:hover {
        color: white;
        transform: scale(1.2);
        padding: 2px 6px;
        border-radius: 4px;
      }
      .github:hover {
        background-color: #3a383c;
      }
      .linkedin:hover {
        background-color: #0a66c2;
      }
      .instagram:hover {
        background-color: #e1306c;
      }
      .twitter:hover {
        background-color: #14171A;
      }
    </style>
</head>
